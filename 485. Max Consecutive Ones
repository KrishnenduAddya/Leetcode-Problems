// Amazon Google Oracle Paym

public class Solution {
    public int FindMaxConsecutiveOnes(int[] nums) {
        int maxOnes = 0;
        int ones = 0;
        int n = nums.Length;
        for(int i = 0; i < n; i++)
        {
            if(nums[i] == 1)
            {
                ones++;
                if(ones > maxOnes)
                    maxOnes = ones;
            }
            else
                ones = 0;
        }
        return maxOnes;
    }
}
