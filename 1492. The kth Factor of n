public class Solution {
    public int KthFactor(int n, int k) {
        int count = 0;
        for(int i = 1; i <= n; i++)
        {
            if(n % i == 0)
                count++;
            if(count == k)
                return i;
        }
        return -1;
    }
}

Another approach upto n / 2

public class Solution {
    public int KthFactor(int n, int k) {
        for(int i = 1; i <= n/2; i++)
        {
            if(n % i == 0 && --k == 0) 
                return i;
        }
        if(k == 1) 
            return n;
        return -1;
    }
}
