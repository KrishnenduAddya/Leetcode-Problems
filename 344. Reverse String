// Amazon Microsoft

// Two Pointer

public class Solution {
    public void ReverseString(char[] s) {
        int left = 0, right = s.Length - 1;
        while(left < right)
        {
            char ch = s[left];
            s[left] = s[right];
            s[right] = ch;
            left++;
            right--;
        }
    }
}
