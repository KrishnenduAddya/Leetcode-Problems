// Prefix Sum

public class NumArray {
    // Array to store prefix sums, where prefixSum[i] = Sum of nums[0..i-1]
    private int[] prefixSum;

    public NumArray(int[] nums) {
        int n = nums.Length;
        prefixSum = new int[n + 1]; // Extra space so prefixSum[0] = 0
        prefixSum[0] = 0;           

        // Build prefixSum array
        for (int i = 0; i < n; i++) {
            // prefixSum[i+1] holds sum of nums[0..i]
            prefixSum[i + 1] = prefixSum[i] + nums[i];
        }
    }
    
    // Returns sum of elements between indices left and right (inclusive)
    public int SumRange(int left, int right) {
        return prefixSum[right + 1] - prefixSum[left];
    }
}

/**
 * Your NumArray object will be instantiated and called as such:
 * NumArray obj = new NumArray(nums);
 * int param_1 = obj.SumRange(left,right);
 */
