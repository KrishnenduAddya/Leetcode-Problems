public class Solution {
    public string ReverseWords(string s) {
        int n = s.Length;
        string result = string.Empty;
        for(int i = 0; i < n; i++)
        {
            while(i < n && s[i] == ' ')
                i++;
            if(i >= n)
                break;
            int start = i;
            while (i < n && s[i] != ' ')
                i++;
            string sub = s.Substring(start, i - start);
            if(string.IsNullOrEmpty(result))
                result = sub;
            else
                result = sub + " " + result;
        }
        return result;
    }
}
