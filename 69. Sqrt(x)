// Amazon Google Goldman Sachs

public class Solution {
    public int MySqrt(int x) {
        long low = 1, high = x, ans = 0;

        while(low <= high)
        {
            long mid = low + (high - low) / 2;
            long square = mid * mid;
            if(square <= x)
            {
                ans = mid;
                low = mid + 1;
            }
            else
                high = mid - 1;
        }
        return (int)ans;
    }
}
